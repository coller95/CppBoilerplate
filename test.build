# test.build - Makefile fragment for test-related targets
.PHONY: test
.PHONY: test-clean

test:
	cd tests && $(MAKE) test

test-clean:
	cd tests && $(MAKE) clean

test-run-%:
	cd tests && $(MAKE) test-run-$*	

test-make-%:
	cd tests && $(MAKE) test-make-$*

test-clean-%:
	cd tests && $(MAKE) test-clean-$*